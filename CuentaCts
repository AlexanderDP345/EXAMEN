public class CuentaCts { 
    private final double TasaInteresAnual = 0.07; 
    private final double Puntos;
    public CuentaCTS(double Puntos) {
        this.puntos = puntos;
    }
    @Override
    public String toString() {
        return "CuentaCts{" +
                "tasaInteresAnual=" + tasaInteresAnual +
                ", puntos=" + puntos +
                '}';
    }
}